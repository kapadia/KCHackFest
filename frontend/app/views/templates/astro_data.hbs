<div class='astro-data'>
  <link href='http://fonts.googleapis.com/css?family=Carrois+Gothic|Numans' rel='stylesheet' type='text/css'>
  
  <div class="hero-unit">
    <p class="text-center">
      <img class="logo" src="./images/astroviz_logo.png"></img>
    </p>
  </div>
  
  <article class='first'>
    <div class='pixel-info'>
      <table>
        <tr>
          <td>X</td>
          <td data-type='x'>-</td>
        </tr>
        <tr>
          <td>Y</td>
          <td data-type='y'>-</td>
        </tr>
        <tr>
          <td>RA</td>
          <td data-type='ra'>-</td>
        </tr>
        <tr>
          <td>Dec</td>
          <td data-type='dec'>-</td>
        </tr>
        <tr>
          <td>R</td>
          <td data-type='r'>-</td>
        </tr>
        <tr>
          <td>G</td>
          <td data-type='g'>-</td>
        </tr>
        <tr>
          <td>B</td>
          <td data-type='b'>-</td>
        </tr>
      </table>
    </div>
    <label>
      &alpha;
      <input data-type="q" type="range" min="0.01" max="0.4" step="0.01" value="0.03"/>
    </label>
    <label>
      Q
      <input data-type="alpha" type="range" min="0.01" max="1" step="0.01" value="0.01"/>
    </label>
    
    <div class='metadata'></div>
  </article>
  
  <article class='middle'>
    <div class='image'></div>
    {{> overlay}}
  </article>
  
  <article class='last'>
    <div class='histograms'>
      <div class='histogram histogram-i'></div>
      <div class='histogram histogram-r'></div>
      <div class='histogram histogram-g'></div>
    </div>
    
    <div class='metadata'></div>
  </article>
  
</div>
<script type="text/javascript" src="javascripts/astro_data.js"></script>
